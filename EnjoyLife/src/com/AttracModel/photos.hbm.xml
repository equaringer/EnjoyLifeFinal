<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.AttracModel.PhotoBean" table="photos">
        
        
        <id name="photono" column="photono" type="integer" />
        
        <property name="photoname" column="photoname" type="string" />
        <property name="photodata" column="photodata" type="string" />
        <property name="attracno" column="AttracNo" type="integer" />     
    </class>
</hibernate-mapping>