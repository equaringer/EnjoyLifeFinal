<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.AttracModel.AttracBean" table="attrac">
        
        
        <id name="rownumber" column="AttracNo" type="integer" />
        
        <property name="stitle" column="stitle" type="string" />
        <property name="time" column="time" type="string" />
        <property name="xbody" column="xbody" type="string" />
        <property name="cat1no" column="cate1no" type="integer" />
        <property name="cat2no" column="cate2no" type="integer" />
        <property name="mrt" column="mrt" type="string" />
        <property name="info" column="info" type="string" />
        <property name="countyno" column="countyno" type="integer" />
        <property name="distno" column="distno" type="integer" />
        <property name="address" column="addr" type="string" />
      
    </class>
</hibernate-mapping>